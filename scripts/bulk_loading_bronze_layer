use DataWarehouseAnalytics;

TRUNCATE TABLE bronze.crm_cust_info ;
BULK INSERT bronze.crm_cust_info 
FROM 'C:\Users\Public\dataset\crm_cust_info.csv'
WITH (
	FIRSTROW = 2,
	FIELDTERMINATOR = ',',
	TABLOCK
);
GO

